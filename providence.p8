version 15
__lua__
function new_backdrop()
	local backdrop = {y = 0}

 function backdrop:update()
 	local speed = wav(gt,0.7,60)
 	self.y += speed
 end
 
 function backdrop:draw()
		cls()
		map(32,0,0,-64+self.y,16,32)
	end

 return backdrop
end

function _init()
	palt(0,false)
	palt(14,true)
	gt = 0
	backdrop = new_backdrop()
end

function _update()
	gt += 1
	backdrop:update()
end

function wav(val,amp,len)
	return amp*sin(val/len)
end

function _draw()
	backdrop:draw()
	map(0,0,0,0,16,16)
	spr(10,40,-2+wav(gt+40,2.98,60),6,4)
	spr(006,48,3+wav(gt,1.28,60),4,4)
end
