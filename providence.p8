	cls(15)
	map(32,0,-6+bgx,0,32,16)
	map(0,0,0,-1+wav(t-10,1.98,60),16,4)
	map(16,0,0,0,16,16)
	map(0,4,0,32,16,12)
	spr(006,48,2+wav(t,1.98,60),4,4)
end
