pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- enemy behaviour ============

movebad = {}
drawbad = {}

-- level loading ==============

function loadroom(n)
	bads = {}
	play = true

	local rx = n%8 * 16
	local ry = flr(n/8) * 16

	for x = 0,16 do
		for y = 0,16 do
			local f = mget(rx+x, ry+y)

			if fget(f, 0) then
   	add(bads,{f=f,x=x*8,y=y*8})
			end
		end
	end
end

-- main callbacks =============

function _init()
	play = 0
end

function _update()
	if(play!=true)return movesel()
	
	for bad in all(bads) do
		--movebad[bad.f](bad)
	end
end

function movesel()
 if btnp"4" then
		play = (play + 1) % 32
	elseif btnp"5" then
		loadroom(play)
	end
end

function _draw()
	cls()
	if(play!=true)return drawsel()

	for bad in all(bads) do
		--drawbad[bad.f](bad)
		spr(bad.f, bad.x, bad.y)
	end
end

function drawsel()
	local rx = play%8 * 16
	local ry = flr(play/8) * 16
	map(rx,ry,0,0,16,16)
end
