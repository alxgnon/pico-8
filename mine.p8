pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
function gamestate()
	return {
		init=init,
	}
end

function _init()
	gt=0
	px,py=4,3
	camy,_camy=0,0
	crystal=40
	crystals={}
	mset(px,py,001)
end

function _update()
	local moved=false
	local _px,_py=px,py

	-- arrow keys
	if btn"3" then
		py+=1
		moved=true
	elseif btn"1" then
		px+=1
		moved=true
	elseif btn"0" then
		px-=1
		moved=true
	elseif btn"2" then
		py-=1
		moved=true
	end

	local neigh=nil
	for c in all(crystals) do
		local dx=c.x-px
		local dy=c.y-py
		if (abs(dx*dx+dy*dy)<9) then
			neigh=c
		end
	end

	if btn"4" and mget(px,py)==002 then
		-- dig with crystal
		c=neigh
		for i=c.y-3,c.y+3 do
			local extent=3
			if(i==c.y-3 or i==c.y+3)extent=2

			for j=c.x-extent,c.x+extent do
					local hardness=3

					target=mget(j,i)
					-- hardness of ground
					if target>60 then
						hardness=9
					elseif target>55 then
						hardness=10
					elseif target>27 then
						hardness=3
					elseif target>24 then
						hardness=4
					elseif target>19 then
						hardness=1
					elseif target>15 then
						hardness=2
					elseif target>11 then
						hardness=1
					elseif target>8 then
						hardness=2
					end

					-- hardness applied
					if gt%hardness!=0 then
						moved=false
					end

					-- dig ground
					if fget(target,0) then
						if target>7 then
							if(target==15)target=-1
							if(target==23)target=-1
							if(target==31)target=15
							if(target==63)target=28
							if (target>55) then
								crystal+=1
								c.pow=min(c.pow+1,27)
							end
							mset(j,i,target+1)
						end
					end

			end
		end
	elseif btn"4" and crystal>=10 then
		if not neigh and mget(px,py)<=001 then
			add(crystals,{x=px,y=py,pow=8,t=-1})
			mset(px,py,002)
			crystal-=10
			moved=false
		end
	end

	local target=mget(px,py)
	local hardness=3

	-- hardness of ground
	if target>60 then
		hardness=9
	elseif target>55 then
		hardness=10
	elseif target>27 then
		hardness=3
	elseif target>24 then
		hardness=4
	elseif target>19 then
		hardness=1
	elseif target>15 then
		hardness=2
	elseif target>11 then
		hardness=1
	elseif target>8 then
		hardness=2
	end

	-- hardness applied
	if gt%hardness!=0 then
		moved=false
	end

	-- dig ground
	if fget(target,0) and moved then
		if target>7 then
			if (target>55) then
				crystal+=1
			end
			if(target==15)target=-1
			if(target==23)target=-1
			if(target==31)target=15
			if(target==63)target=28
			mset(px,py,target+1)
			moved=false
		end
	end

	-- game bounds
	if px<0 or px>15 or py<0 then
		moved=false
	end

	-- only move if clear
	if moved then
		mset(_px,_py,000)
		mset(px,py,001)
	else
		px,py=_px,_py
	end

	gt+=1

	-- camera targeting
 if py*8>camy+48 then
 	camy+=24
	elseif py*8<camy+16 then
		camy-=24
	end

	-- camera movement
	if _camy<camy then
		_camy+=2
	elseif _camy>camy then
		_camy-=3
	end

	-- put buildings on top
	for c in all(crystals) do
		if px==c.x and py==c.y and btn"4" and crystal>0 then
			c.pow+=0.5
		end
		mset(c.x,c.y,002)
		-- also, shooting
		if c.pow > 20 and crystal>0 then
			local depth=c.y+12
			for i=c.y,depth do
				local tile=mget(c.x,i)
				if tile>7 and tile<32 then
					tile-=7
					if tile<8 and i>depth-4 then
						tile=63
					elseif tile<8 then
						tile=0
					end
				elseif tile>56 and i>depth-4 then
					tile-=1
				end
				mset(c.x,i,tile)
			end
			c.pow-=10
			crystal-=1
		end
	end
end

function _draw()
	cls()
	camera(0,_camy)
	for c in all(crystals) do
		circ(c.x*8+3,c.y*8+3,c.pow,12)
		if c.pow>18 then
			rectfill(c.x*8+2,c.y*8+10,c.x*8+4,c.y*8+96)
		end
	end
	map(0,0,0,0,16,64)
	camera()
	rectfill(112,0,128,8,0)
	print(crystal,114,2,12)
end
