---------------- constants ----

-- sound effects
sound =
{ jump = 00
}

-- sprites
sp =
{ player = 064
, board = 081
}

-- merges tables b into a
function merge(a, b)
	if b then
		for k, v in pairs(b) do
			a[k] = v
		end
	end
	return a
end
