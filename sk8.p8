---------------- game loop ----

br = board{}
pl = player{grab=br}

function _init()
	for y = 0,63 do for x = 0,127 do
		if mget(x,y) == sp.player then
			pl.x,pl.y = x,y+1
			return
		end
	end end
end

function _update()
	pl:update()
	br:update()
end

function _draw()
	cls(13)
	camera(mid(0,pl.x*8-64,896),0)
	mapdraw(0,0,0,0,128,64,1)
	pl:draw()
	br:draw()
end
