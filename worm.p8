__lua__

-- worm =======================

function make_worm(tile)
	local x,y=find(tile)
	return{x=x,y=y,tail={}}
end

function find(tile)
	for i=0,127 do
		for j=0,63 do
			if	mget(i,j)==tile then
				return i,j
			end
		end
	end
end

function move_worm(a)
	if aim==0 then
		try_worm(a,a.x-1,a.y)
	elseif aim==1 then
		try_worm(a,a.x+1,a.y)
	elseif aim==2 then
		try_worm(a,a.x,a.y-1)
	else
		try_worm(a,a.x,a.y+1)
	end
end

function try_worm(a, x, y)
	local tile=mget(x,y)
	if fget(tile,0)or fget(tile,2)then
		x,y = a.x,a.y
		mset(x,y,056+turn%2)
		return
	end

	if a.dig then
		add(a.tail,{x=x,y=y,f=a.dig})
		a.dig = nil
	end
	if fget(tile,3) then
		a.dig = tile+16
	end

	local bx,by = a.x,a.y
	for b in all(a.tail) do
		local tx,ty = b.x,b.y
		b.x,b.y = bx,by
		mset(bx,by,b.f)
		bx,by = tx,ty
	end
	mset(bx,by,000)
	a.x,a.y = x,y
	mset(x,y,056+turn%2)
end

-- main =======================

function _init()
	aim = 1
	turn = 0
	worm = make_worm(056)
end

function getaim()
	if (btn"0") aim = 0 -- left
	if (btn"1") aim = 1 -- right
	if (btn"2") aim = 2 -- up
	if (btn"3") aim = 3 -- down
end

function _update()
	getaim()
	turn += 0.25
	if flr(turn)==turn then
		move_worm(worm)
	end
end

function _draw()
	cls()
	camera(worm.x*8-60,worm.y*8-60)
	map(0,0,0,0,128,64)
end
