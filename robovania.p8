pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
-- globals --------------------

actors = {}
player = nil


-- actor builders -------------

function gameobject(a)
	return {
			x = a.x or 0,
			y = a.y or 0,
			dx = a.dx or 0,
			dy = a.dy or 0,
			accel = a.accel or 0,
			spr = a.spr or 16,
			frame = a.frame or 0,
			t = a.t or 0,
			inertia = a.inertia or 0.6,
			bounce = a.bounce or 1,
			w = a.w or 0.4,
			h = a.h or 0.4,
	}
end

function spawn(a)
	local a = gameobject(a)
	add(actors, a)
	return a
end


-- init -----------------------

function _init()
	player = spawn{x=3, y=3, spr=17,
		inertia=0, bounce=0,
		accel = 0.18}
end


-- collision ------------------

-- is map tile solid?
function solid(x,y)
	return fget(mget(x,y), 1)
end

-- does rect overlap any solids?
-- todo: support large actors
function solidarea(x,y, w,h)
	return
		solid(x-w,y-h) or
		solid(x+w,y-h) or
		solid(x-w,y+h) or
		solid(x+w,y+h)
end

function collidex(a)
	if not solidarea(
			a.x+a.dx,a.y, a.w,a.h) then
		a.x += a.dx
	else
		a.dx *= -a.bounce
	end
end

function collidey(a)
	if not solidarea(
			a.x,a.y+a.dy, a.w,a.h) then
		a.y += a.dy
	else
		a.dy *= -a.bounce
	end
end


-- physics --------------------

function inertia(a)
	a.dx *= a.inertia
	a.dy *= a.inertia
end


-- player control -------------

function control(a)
	a.b0=btn(0) a.b1=btn(1)
	a.b2=btn(2) a.b3=btn(3)

	if (a.b0) a.dx -= a.accel 
	if (a.b1) a.dx += a.accel
	if (a.b2) a.dy -= a.accel
	if (a.b3) a.dy += a.accel
end


-- animate --------------------

function animplayer(a)
	if not (a.b0 or a.b1
			or a.b2 or a.b3) then
		-- idle
		if a.t > (a.eyeoff or 0) then
			a.frame = 0
		else
			a.frame = 1
		end

	else
		-- moving
		a.eyeoff = a.t + 40
		if a.b2 then
			a.frame = 2
		elseif a.b3 then
			a.frame = 3
		else
			a.frame = 1
		end

		if a.b0 then
			a.flipx = true
		elseif a.b1 then
			a.flipx = false
		end
	end
end


-- update ---------------------

function act(a)
	a.t += 1
	collidex(a)
	collidey(a)
	inertia(a)
end

function _update()
	control(player)
	animplayer(player)
	foreach(actors, act)
end


-- draw -----------------------

function drawactor(a)
	local sx = a.x * 8 - 4
	local sy = a.y * 8 - 4
	spr(a.spr+a.frame, sx,sy, 1,1,
		a.flipx)
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
	foreach(actors, drawactor)
end
