-- player =====================
function new_player(x, y)
	return {
		x = x, y = y,
		move = move_player,
		draw = draw_player
	}
end

function move_player(a)
	local dx, dy = 0, 0
	if (btn"0") dx -= 2
	if (btn"1") dx += 2
	if (btn"2") dy -= 2
	if (btn"3") dy += 2
	a.x = min(max(a.x+dx,0),120)
	a.y = min(max(a.y+dy,0),121)

	if not btn "4" then
		if (dx > 0) a.f = false
		if (dx < 0) a.f = true
	end
end

function draw_player(a)
	spr(0, a.x, a.y, 1, 1, a.f)
end

-- baddies ====================
new_baddy = {}

-- room =======================
function new_room(n)
	local a = {
		baddies = {},
		player = new_player(8, 56),
		move = move_room,
		draw = draw_room
	}
	load_room(a, level_coords(n))
	return a
end

function spawn(a, g, x, y)
	local b = new_baddy[g](x, y)
	add(a.baddies, b)
end

function load_room(a, rx, ry)
	for x = 0,16 do
		for y = 0,16 do
			local g = mget(rx+x, ry+y)
			if fget(g, 0) then
				spawn(a, g, x*8, y*8)
			end
		end
	end
end

function move_room(a)
	a.player:move()
	for b in all(a.baddies) do
		b:move()
	end
end

function draw_room(a)
	rect(0,0,127,127,1)
	a.player:draw()
	for b in all(a.baddies) do
		b:draw()
	end
end

-- select =====================
function new_select()
	return {
		n = 0,
		move = move_select,
		draw = draw_select
	}
end

function move_select(a)
	if btnp"4" then
		state = new_room(a.n)
	elseif btnp"5" then
		a.n = (a.n + 1) % 32
	end
end

function level_coords(n)
	local x = n % 8 * 16
	local y = flr(n / 8) * 16
	return x, y
end

function draw_select(a)
	local x, y = level_coords(a.n)
	map(x, y, 0, 0, 16, 16)
	print(a.n)
end

-- callbacks ==================
function _init()
	palt(0, false)
	palt(1, true)
	state = new_select()
end

function _update()
	state:move()
end

function _draw()
	cls()
	state:draw()
end
