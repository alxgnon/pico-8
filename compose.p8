pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
-- compose
-- a tale about computer systems

const = {

	-- tiles
	ruction = 1,

	-- sprites
	nobody = 64,

}

function load_systems()
	update_systems = {
		timer,
		buttons,
		joymove,
		delta,
		room,
	}

	draw_systems = {
		sky,
		room,
		sprite,
	}

	systems = {}
	concat(systems, update_systems)
	concat(systems, draw_systems)
end

function _init()
	load_systems()

	spawn{sky = {}}
	spawn{room = {}, x = 0, y = 0}

	-- player
	spawn{
		sprite = const.nobody,
		x = 2, y = 2,
		dx = 0, dy = 0,
		speed = 0.125,
		buttons = {},
	}
end

function _update()
	for system in all(update_systems) do
		for a in all(system.actors) do
			system.update(a)
		end
	end
end

function _draw()
	for system in all(draw_systems) do
		for i = #system.actors, 1, -1 do
			system.draw(system.actors[i])
		end
	end
end
-->8
-- common ---------------------
-- generally useful functions

-- add actor to matching systems
function spawn(a)
	for system in all(systems) do
		if system.matches(a) then
			addi(system.actors, a)
		end
	end
end

-- remove actor from all systems
function kill(a)
	for system in all(systems) do
		deli(system.actors, a)
	end
end

-- merges tables b into a
function merge(a, b)
	if b then
		for k, v in pairs(b) do
			a[k] = v
		end
	end
	return a
end

-- concatenates tables
function concat(a, b)
	for i = 1, #b do
		a[#a + 1] = b[i]
	end
	return a
end

-- indexed add
function addi(tbl, v)
	if not tbl[v] then
		local i = #tbl + 1
		tbl[v] = i
		tbl[i] = v
	end
end

-- indexed delete
function deli(tbl, v)
	if tbl[v] then
		tbl[v] = nil
		del(tbl, v)
	end
end

-- get corner from tile center
function get_corner(a)
	return a.x*8-4, a.y*8-4
end

-- snap x to grid
function snap(x, grid)
	return flr(x / grid) * grid
end

-->8
-- systems --------------------

-- timers count down 1 per frame
timer = {actors = {}}

function timer.matches(a)
	return a.timer
end

function timer.update(a)
	for k, _ in pairs(a.timer) do
		a.timer[k] -= 1
	end
end

-- clears the screen
sky = {actors = {}}

function sky.matches(a)
	return a.sky
end

function sky.draw(a)
	cls(a.sky.color or 0)
end

-- draws map tiles
room = {actors = {}}

function room.matches(a)
	return a.room and a.x and a.y
end

function room.update(a)
	local player = joymove.actors[1]

	if player then
		a.x = snap(player.x, 16)
		a.y = snap(player.y, 16)
	end
end

function room.draw(a)
	local x, y = a.x * 8, a.y * 8
	camera(x, y)
	map(a.x, a.y, x, y, 16, 16)
end

-- draws a sprite
sprite = {actors = {}}

function sprite.matches(a)
	return a.sprite and a.x and a.y
end

function sprite.draw(a)
	local frame = a.frame or 0
	local sprite = a.sprite + frame
	local x, y = get_corner(a)

	spr(sprite, x, y, 1, 1,
			a.flip_x, a.flip_y)
end

-- listens to player input
buttons = {actors = {}}

function buttons.matches(a)
	return a.buttons
end

function buttons.update(a)
	local b = a.buttons
	b.l, b.r = btn"0", btn"1"
	b.u, b.d = btn"2", btn"3"
	b.o, b.x = btn"4", btn"5"
end

-- sets dx,dy from buttons
joymove = {actors = {}}

function joymove.matches(a)
	return a.buttons and a.speed
end

function joymove.update(a)
	local b = a.buttons
	a.dx, a.dy = 0, 0

	if (b.l) a.dx -= a.speed
	if (b.r) a.dx += a.speed
	if (b.u) a.dy -= a.speed
	if (b.d) a.dy += a.speed
end

-- moves actor by dx,dy
delta = {actors = {}}

function delta.matches(a)
	return a.dx and a.dy
end

function delta.update(a)
	a.x += a.dx
	a.y += a.dy
end
