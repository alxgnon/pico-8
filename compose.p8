pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
-- sp -------------------------
-- holds sprite constants

sp = {
	object = 64,
}


-- do_nothing -----------------
-- does absolutely nothing

function do_nothing()
end


-- merge ----------------------
-- merges tables b into a

function merge(a, b)
	if b then
		for k, v in pairs(b) do
			a[k] = v
		end
	end
	return a
end


-- scene ----------------------
-- handles actors

local function draw_scene(s)
	for a in all(s.actors) do
		a:draw()
	end
end

function scene(o)
	local s = {
		init = do_nothing,
		update = do_nothing,
		draw = draw_scene,
	}
	return merge(s, o)
end


-- room -----------------------
-- acts a room in the map

local function draw_room(a)
	map(a.x,a.y,a.x*8,a.y*8,16,16)
end

function room(o)
	local a = {
		x = 0, y = 0,
		init = do_nothing,
		update = do_nothing,
		draw = draw_room,
	}
	return merge(a, o)
end


-- object ---------------------
-- acts a posable object

local function get_corner(a)
	return a.x*8-4, a.y*8-4
end

local function draw_object(a)
	local f = a.frame or 0
	local x, y = get_corner(a)
	spr(a.sprite + f, x, y, 1, 1,
			a.flip_x, a.flip_y)
end

function object(o)
	local a = {
		x = 0, y = 0,
		sprite = sp.object,
		init = do_nothing,
		update = do_nothing,
		draw = draw_object,
	}
	return merge(a, o)
end


-- game -----------------------
-- handles the game state

local function init_game(g)
	g.room = room()
	g.object = object{x=2,y=2}
end

local function update_game(g)
end

local function draw_game(g)
	cls()
	g.room:draw()
	g.object:draw()
end

function game(o)
	local g = {
		init = init_game,
		update = update_game,
		draw = draw_game,
	}
	return merge(g, o)
end


-- state ----------------------
-- ties callbacks to game state

state = game()

function _init()
	state:init()
end

function _update()
	state:update()
end

function _draw()
	state:draw()
end
