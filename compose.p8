pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
function do_nothing()
	-- zzz...
end

-- const ----------------------

const = {
	-- tiles
	ruction = 1,
	
	-- sprites
	nobody = 64,
}


-- merge ----------------------
-- merges tables b into a

function merge(a, b)
	if b then
		for k, v in pairs(b) do
			a[k] = v
		end
	end
	return a
end


-- scene ----------------------
-- handles actors

function init_scene(s)
	s.actors = {}
end

function add_to_scene(s, a)
	add(s.actors, a)
end

function update_scene(s)
	for a in all(s.actors) do
		a:update()
	end
end

function draw_scene(s)
	for a in all(s.actors) do
		a:draw()
	end
end

function scene(o)
	local s = {
		actors = {},
		init = init_scene,
		add = add_to_scene,
		update = update_scene,
		draw = draw_scene,
	}
	return merge(s, o)
end


-- room -----------------------
-- the current room

local function draw_room(a)
	map(a.x,a.y,a.x*8,a.y*8,16,16)
end

function room(o)
	local a = {
		x = 0, y = 0,
		init = do_nothing,
		update = do_nothing,
		draw = draw_room,
	}
	return merge(a, o)
end


-- object ---------------------
-- a basic game object

-- get corner from tile center
function get_corner(a)
	return a.x*8-4, a.y*8-4
end

function draw_object(a)
	local f = a.frame or 0
	local x, y = get_corner(a)
	spr(a.sprite + f, x, y, 1, 1,
			a.flip_x, a.flip_y)
end

function object(o)
	local a = {
		x = 0, y = 0,
		sprite = const.nobody,
		init = do_nothing,
		update = do_nothing,
		draw = draw_object,
	}
	return merge(a, o)
end


-- ctl ------------------------
-- updates object with buttons

function update_ctl(a)
	a.left,a.right=btn"0",btn"1"
	a.up,a.down=btn"2",btn"3"
	a.bo,a.bx=btn"4",btn"5"
end

function ctl(a)
	local update = a.update

	a.update = function (a)
		update_ctl(a)
		update(a)
	end
	return a
end


-- game -----------------------
-- handles the game state

function init_game(g)
	g.scene = scene()
	g.scene:add(room())
	
	g.pl = object{x=2,y=2}
	g.pl = ctl(g.pl)
	g.scene:add(g.pl)
end

function update_game(g)
	g.scene:update()
end

function draw_game(g)
	cls()
	g.scene:draw()
end

function game(o)
	local g = {
		init = init_game,
		update = update_game,
		draw = draw_game,
	}
	return merge(g, o)
end


-- state ----------------------
-- ties callbacks to game state

state = game()

function _init()
	state:init()
end

function _update()
	state:update()
end

function _draw()
	state:draw()
end
