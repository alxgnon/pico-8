player_colors = {9, 13}

aim_sens = 0.005
power_sens = 0.03
min_power = 0.5
max_power = 2.5

player_radius = 3
gun_length = 9

function aim_center(x, y)
	return atan2(64-x, 64-y, x, y)
end

function player(x, y, col)
	local d = aim_center(x, y)

	return {
		x = x,
		y = y,
		col = col,
		d = d,
		old = d,
		r = player_radius,
		power = max_power / 2,
	}
end

_btnpp = {}
function btnpp(n)
	if btn(n) then
		local t = (_btnpp[n] or 0) + 1
		_btnpp[n] = t
		return btnp(n) or t > 40
	else
		_btnpp[n] = 0
	end
end

function move_player(a)
	if (not a) return

	a.old = a.d
	if (btnpp"0") a.d+=aim_sens
	if (btnpp"1") a.d-=aim_sens
	if a.old != a.d then
		sfx(2)
	end

	local pwr = a.power
	if (btnpp"2") pwr += power_sens
	if (btnpp"3") pwr -= power_sens
	pwr = mid(pwr,min_power,max_power)
	if pwr != a.power then
		sfx(1, 3, pwr * 2, 1)
		a.power = pwr
	end
end

function draw_players()
	for i, a in pairs(players) do
		circfill(a.x,a.y,a.r+1,0)
		draw_gun(a.x,a.y,a.old,0)
		draw_gun(a.x,a.y,a.d,a.col)

		circfill(a.x,a.y,a.r,a.col)

		if i == playing then
			circfill(a.x, a.y, 1, 0)
		end
	end
end

function draw_gun(x, y, d, col)
	line(
			x, y,
			x + cos(d) * gun_length,
			y + sin(d) * gun_length,
			col)
end
