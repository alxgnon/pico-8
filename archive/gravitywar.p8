vector_mt = {
	__add = function(a,b)
		return {x=a.x+b.x, y=a.y+b.y}
	end,
}

function vec(x, y)
	local v = {x=x,y=y}
	setmetatable(v, vector_mt)
	return v
end

function vec_to_string(v)
	return "("..v.x..","..v.y..")"
end

