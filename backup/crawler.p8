pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- crawler
--
function _init()
	palt(0,false)
	palt(1,true)
	init_player(32,0)
	gt=0
end

function init_player(x,y)
	px,py,cx,cy,dx,dy,f,sf=
		x,y,0,0,0,0,false,false
	floor,jrelease=false,false
end

function update_player()
	if btn"5" and not thrust then
		if mget(px/8,py/8)==46 then
			mset(px/8,py/8,47)
			for i=0,128 do
				for j=0,64 do
					if fget(mget(i,j),1) then
						mset(i,j,0)
					end
				end
			end
		end
		thrust=true
	end
	if btn"5" and thrust then
		dx=(f and -1 or 1)*8
		dy=0
	elseif thrust then
		thrust=false
		slash=true
	else
		if floor and not btn"4" then
			jrelease=true
		end
		bx=0
		if(btn"0")bx,f=bx-1,true
		if(btn"1")bx,f=bx+1,false
		if btn"4" and not btn"3" and floor and jrelease and dy>=0 then
			jrelease,dy=false,-5
		elseif dy<-1 then
			floor,dy=false,dy+0.5
			if(not btn"4")dy=3
		else
			floor,dy=false,4
		end
		if bx!=0 and abs(dx)<4 then
			dx+=bx
		elseif abs(dx)>1 then
			dx-=sgn(dx)
		else
			dx=0
		end
	end
	-- move left
	if dx<0 then
		for i=-1,dx,-1 do
			t=mget((px+i)/8,(py+8)/8)
			if px<1 or fget(t,0) then
				if thrust then
					thrust=false
					slash=true
					jrelease,floor=true,true
				else
					slash=false
				end
				break
			end
			px,sf=px-1,true
		end
		-- move right
	elseif dx>0 then
		for i=1,dx do
			t=mget((px+i+8)/8,(py+8)/8)
			if fget(t,0) then
				if thrust then
					thrust=false
					slash=true
					jrelease,floor=true,true
				else
					slash=false
				end
				break
			end
			px,sf=px+1,false
		end
	end
	-- move down
	if dy>0 then
		for i=1,dy do
			t=mget(px/8,(py+i+8)/8)
			if fget(t,0) or fget(t,1) and
					not (btn"3" and btn"4") then
				floor=true
				break
			elseif dx then
				t=mget((px+8)/8,(py+i+8)/8)
				if fget(t,0) or fget(t,1) and
						not (btn"3" and btn"4") then
					floor=true
					break
				end
			end
			py+=1
		end
	end
	-- move up
	if dy<0 then
		for i=-1,dy,-1 do
			t=mget(px/8,(py-i)/8)
			if fget(t,0) then
				break
			elseif dx then
				t=mget((px+8)/8,(py-i)/8)
				if fget(t,0) then
					break
				end
			end
			py-=1
		end
	end
end

function _update()
	gt+=1
	update_player()
	px=max(px,0)
	py=max(py,0)
end

-- draw
--
function _draw()
	cls()
	draw_camera()
	draw_palette()
	if gt%56==0 then
		for i=ccx,ccx+16 do
			for j=ccy,ccy+8 do
				t=mget(i,j)
				if fget(t,3) then
					even=flr(t/2)*2
					mset(i,j,t==even and even+1 or even)
				end
			end
		end
	elseif gt%56==28 then
		for i=ccx,ccx+16 do
			for j=ccy+8,ccy+16 do
				t=mget(i,j)
				if fget(t,3) then
					even=flr(t/2)*2
					mset(i,j,t==even and even+1 or even)
				end
			end
		end
	end
	map(ccx,ccy,cx,cy,16,16,7)
	t=(floor and dx!=0) and
		67+(gt/4)%2 or 66
	spr(t,px,py,1,1,f)
	sx=8*(sf and 1 or -1)
	if slash then
		spr(65,px-sx,py,1,1,not sf)
	else
		spr(65,px+sx,py,1,1,sf)
	end
	map(cx/8,cy/8,cx,cy,16,16,8)
end

-- draw camera
--
function draw_camera()
	_cx,_cy=cx,cy
	cx=flr(px/128)*128
	cy=flr(py/128)*128
	ccx,ccy=flr(cx/8),flr(cy/8)
	camera(cx,cy)
end

-- draw palette
--
function draw_palette()
	if _cx!=cx then
		if cx>=768 then
			pal(5,0)
			pal(11,8)
		elseif cx>=512 then
			pal(5,2)
			pal(11,6)
		elseif cx>=256 then
			pal(5,4)
			pal(11,6)
		else
			pal(5,5)
			pal(11,11)
		end
	end
end
