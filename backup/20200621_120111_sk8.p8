------------------- board ----

function board(opts)
	return merge{object
	{ bounce = 0
	, frame = sp.board
	, fric = 0.985
	, airfric = 0.985
	, h = 0.25
	, update = update_board
	, draw = draw_board
	}, opts}
end

function update_board(a)
	if pl.grab ~= a then
		physics(a)
		riding(pl, a)
	end
end

function riding(a, b)
	if a.ride == b then

		if b.slide and b.standing then
			b.dx *= 0.92

			if abs(b.dx) < 0.02 then
				sfx(-1, 3)
			end
		end

		b.d = a.d
		b.x = a.x
		a.y = b.y - 0.25
		a.dx = b.dx
		a.dy = b.dy
	end
end

function draw_board(a)
	animate_board(a)
	draw_sprite(a)
end

function animate_board(a)
	a.frame = sp.board

	if a.slide then
		a.frame = sp.board + 4
	elseif pl.grab == a then
		a.d = pl.d
		a.frame = sp.board - 1
	end
end
