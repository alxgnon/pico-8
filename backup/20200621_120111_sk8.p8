---------------- constants ----

-- sound effects
sound =
{ jump = 00
, land = 01
, push = 02
, olli = 03
, slide = 04
}

-- sprites
sp =
{ player = 064
, board = 081
}

function merge(tables)
	local a = {}
	for b in all(tables) do
	 for k, v in pairs(b) do
			a[k] = v
		end
	end
	return a
end
