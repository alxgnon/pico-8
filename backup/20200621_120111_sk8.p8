--------------- physics ----

-- default components
def =
{ timer =
		{ timer = {}
		}

, body =
		{ x = 0
		, y = 0
		, w = 0.3
		, h = 0.5
		}

, sprite =
		{ frame = 1
		, f0 = 0
		, d = 1
		, oy = 0
		}

, movement =
		{ dx = 0
		, dy = 0
		, ddy = 0.06
		, bounce = 0.8
		, air = true
		}

, friction =
		{ fric = 0.8
		, airfric = 0.9
		}
}

-- basic game object
function object(options)
	return merge
	{ def.timer
	, def.body
	, def.sprite
	, def.movement
	, def.friction
	, options
	}
end

-- timers count down 1 per frame
function update_timers(a)
	for k, _ in pairs(a.timer) do
		a.timer[k] -= 1
	end
end

function physics(a)
	sideways(a)
	upwards(a)
	downwards(a)
	gravity(a)
	friction(a)
end

-- test if a point is solid
function solid(x, y)
	local oy = flr(x/128) * 16
	local tile = mget(x%128, oy+y)

	return x < 0 or x >= 512
			or fget(tile, 1)
end

-- applies x movement
function sideways(a)
	local dx3 = sgn(a.dx) * 0.3
	local x0 = a.x + a.dx + dx3

	if not solid(x0,a.y-0.5) then
		a.x += a.dx
		return
	end

	while not solid(a.x+dx3,a.y-0.5) do
		a.x += sgn(a.dx) * 0.1
	end

	a.dx *= -0.5

	if (pl.ride == a) falloff(pl, a)
end

-- fall off board
function falloff(pl, b)
	pl.ride = nil
	pl.olli = nil
	b.slide = false
	sfx(-1, 3)

	local dx = abs(b.dx)
	if dx > 0.1 then
		pl.timer.oops = dx * 140
	end
end

-- applies upwards y movement
function upwards(a)
	if (a.dy >= 0) return
	local xl,xr=a.x-0.2,a.x+0.2
	local y0 = a.y + a.dy - 1

	if not (solid(xl,y0) or
			solid(xr,y0)) then
		a.y += a.dy
		return
	end

	a.dy = 0

	while not (solid(xl,a.y-1) or
			solid(xr,a.y-1)) do
		a.y -= 0.01
	end
end

-- applies downwards y movement
function downwards(a)
	a.standing = false
	if (a.dy < 0) return
	local xl,xr=a.x-0.2,a.x+0.2
	local y0 = a.y + a.dy

	if not (solid(xl,y0) or
			solid(xr,y0)) then
		a.y += a.dy
		a.air = true
		if (a == br) sfx(-1, 3)
		return
	end

	if a.bounce > 0 and a.dy > 0.2 then
		a.dy *= -pl.bounce
	else
		a.standing = true
		a.dy = 0

		if a == br and a.air then
			sfx(sound.land)
			a.air = false

			if a.slide then
				sfx(sound.slide, 3)
			end
		end
	end

	while not (solid(xl,a.y) or
			solid(xr,a.y)) do
		a.y += 0.05
	end

	while solid(xl,a.y-0.1) do
		a.y-=0.05
	end
	while	solid(xr,a.y-0.1) do
		a.y-=0.05
	end
end

-- applies gravity
function gravity(a)
	a.dy += a.ddy
	a.dy *= 0.95
end

-- applies x friction
function friction(a)
	if a.standing then
		a.dx *= a.fric
		if (abs(a.dx) < 0.01) a.dx = 0
	else
		a.dx *= a.airfric
	end
end

-- check if actors are colliding
function colliding(a, b)
	if (a == b) return
	local dx = a.x - b.x
	local dy = a.y - b.y
	if abs(dx) < a.w+b.w then
		if abs(dy) < a.h+b.h then
			return true
		end
	end
end

-- get corner from sprite origin
function get_corner(a)
	local oy = a.oy or 0
	return a.x*8-4, (a.y+oy)*8-8
end

-- draws sprites
function draw_sprite(a)
	local x, y = get_corner(a)
	spr(a.frame,x,y,1,1,a.d<0)
end
