------------------- player ----

function player(opts)
	return merge{object
	{ bounce = 0
	, timer =
			{ oops=-1
			, push=-1
			}
	, update = update_player
	, draw = draw_player
	}, opts}
end

function update_player(a)
	update_timers(a)
	control_player(a)
	physics(a)
	grabbing(a)
end


function grabbing(a)
	if a.grab then
		local b = a.grab
		b.x = a.x + a.d * 0.75
		b.y = a.y - 0.25
		b.dx = 0
		b.dy = 0
	end
end

function control_player(a)
	if (a.timer.oops > 0) return
	local btn =
			{ l=btn"0", r=btn"1"
			, u=btn"2", d=btn"3"
			, o=btn"4", x=btn"5"
			}

	if a.ride then
		board_control(a, btn)
	else
		ground_control(a, btn)
	end
end

function press(a, btn, key, fn)
	local hold = "hold"..key
	if not btn[key] then
		a[hold] = false
	elseif not a[hold] then
		a[hold] = true
		fn()
	end
end

function board_control(a, btn)
	local flags =
			(btn.l and 1 or 0) +
			(btn.r and 2 or 0) +
			(btn.u and 4 or 0) +
			(btn.d and 8 or 0)

	if flags > 0 then
		press(a, btn, "o", function ()
		 if not a.ride.slide then
		 	sfx(sound.slide, 3)
		 end

		 a.ride.slide = true
		 a.olli = false
		end)

		if not a.ride.slide then
			develop_trick(a, flags)
		end
	else
		if a.ride.standing then
			if a.olli then
				a.ride.dy = -0.7
				sfx(sound.olli)
			end
		end
		a.olli = false
		a.ride.slide = false
		sfx(-1, 3)
	end

	-- push
	press(a, btn, "o", function ()
		if a.ride.standing and
				a.timer.push <= -1 and
				not a.olli and not a.slide then
			sfx(sound.push)
			a.timer.push = 7
			a.dx += a.d * 0.13
			a.ride.dx = a.dx
		end
	end)

	-- dismount
	press(a, btn, "x", function ()
		a.ride = false
		a.olli = nil
		a.slide = false
		sfx(-1, 3)
	end)
end

function develop_trick(a, flags)
	if a.olli ~= nil then
		a.olli = {}
	end
end

function ground_control(a, btn)
	local accel = 0.025
	if (a.standing) accel *= 2

	if btn.l then
		a.dx -= accel
		a.d = -1
	end

	if btn.r then
		a.dx += accel
		a.d = 1
	end

	-- jump
	press(a, btn, "o", function ()
		if a.standing then
			a.dy = -0.7
			sfx(sound.jump)
		end
	end)

	a.contact = nil
	if colliding(a, br) then
		a.contact = br
	end

	-- grab / drop
	press(a, btn, "x", function ()
		if a.grab then
			running_start(a, a.grab)
		elseif a.contact then
			grab_it(a, a.contact)
		end
	end)
end

function running_start(a, b)
	a.ride = b
	b.x = a.x
	b.y = a.y
	b.dx = a.dx * 1.4
	b.dy = a.dy
	a.grab = nil
end

function grab_it(a, b)
	a.grab = b
	b.air = true
end

function draw_player(a)
	animate_player(a)
	draw_sprite(a)
end

function animate_player(a)
	a.oy = 0

	if a.timer.push > 0 then
		a.f0 = 0
		a.frame = sp.player + 4
		return
	end

	if a.olli then
		a.f0 = 0
		a.frame = sp.player + 6
		return
	end

	if a.timer.oops > 0 then
		a.f0 = 0
		a.frame = sp.player + 5
		return
	end

	if abs(a.dx) < 0.1 or a.ride then
		a.f0 = 0
		a.frame = sp.player
		return
	end

	local f1 = a.standing and
			abs(a.dx) * 2 or
			abs(a.dx) / 2

	a.f0 = (a.f0 + f1 + 4) % 4
	a.frame =	sp.player+1+flr(a.f0)

	if a.f0 >= 1 and a.f0 < 3 then
		a.oy = -0.125
	end
end

