pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
-- vania
--
function _init()
	state=game()
end

function _update()
	update_game(state)
end

function _draw()
	draw_game(state)
end

-- game -----------------------

function game()
	return{
		world=world(128,64),
		cam={x=8,y=0},
		hero=hero(32,0)
	}
end

function update_game(a)
	update_hero(a.hero)
	update_cam(a.cam,a.hero)
end

function draw_game(a)
	cls()
	draw_cam(a.cam)
	draw_world(a.world)
	draw_hero(a.hero)
end

-- world ----------------------

function world(w,h)
	for i=0,w-1 do
		for j=0,h-1 do
			if mget(i,j)==1 then
				if rnd(99)>94 then
					mset(i,j,3)
				elseif rnd(99)>76 then
					mset(i,j,2)
				end
			end
		end
	end
	return {w=w,h=h}
end

function draw_world(a)
	map(0,0,0,0,a.w,a.h)
end

-- cam ------------------------

function update_cam(a,b)
	a.x=min(max(b.x-48,8),896)
	a.y=flr(b.y/128)*128
end

function draw_cam(a)
	camera(a.x,a.y)
end

-- hero -----------------------

function hero(x,y)
	return{
		x=x,
		y=y,
		f=48,
		t=0,
		dx=0,
		dy=0,
		air_t=10,
		hat=32+flr(rnd(2))
	}
end

function update_hero(a)
	hoz_input(a)
	vert_input(a)
	hoz_movement(a)
	vert_movement(a)
	update_physics(a)
	animate_hero(a)
	a.t+=1
end

function hoz_input(a)
	if btn(0) and btn(1) then
		a.dx=0
	elseif btn(0) then
		a.dx=-3
		a.z=true
	elseif btn(1) then
		a.dx=3
		a.z=false
	end
end

function vert_input(a)
	local can_jump
	if btn(4) then
		can_jump=a.land or a.air_t<4
		if can_jump and a.release then
				a.land=false
				a.air_t=0
				a.dy=-8
		end
	end
	a.release=not btn(4)
end

function hoz_movement(a)
	local t,tx,ty
	if(abs(a.dx)<1)return
	for i=1,abs(a.dx) do
		a.x+=sgn(a.dx)
		for j=1,6,5 do
			tx=flr((a.x+j)/8)
			ty=flr(a.y/8)
			t=mget(tx,ty)
			if fget(t,1) then
				a.x-=sgn(a.dx)
				a.dx=0
				return
			end
		end
	end
end

function vert_movement(a)
	local t,tx,ty
	local oy=a.y
	local land=a.land
	for i=1,abs(a.dy) do
		a.y+=sgn(a.dy)
		for j=1,6,5 do
			tx=(a.x+j)/8
			ty=a.y/8
			t=mget(tx,ty)
			if fget(t,1) then
				a.y-=sgn(a.dy)
				a.land=true
				return
			end
			a.land=false
		end
	end
	if land and not a.land then
		a.air_t=0
	end
	if a.air_t<3 and oy<a.y then
		a.y=oy
	end
end

function update_physics(a)
	if not a.land then
		a.air_t+=1
	end
	if a.dx!=0 then
		a.dx-=sgn(a.dx)
	end
	if abs(a.dx)<1 then
		a.dx=0
	end
	a.dy=min(a.dy+1,6)
end

function animate_hero(a)
	a.f0=a.f+1
	if a.land and abs(a.dx)>1 then
		a.f0=a.f+flr(a.t/4)%4
	elseif not a.land and a.dy<3 then
		a.f0=a.f+2
	end
	a.hat_y=a.y-(a.f0-1)%2-14
end

function draw_hero(a)
	spr(a.f0,a.x,a.y-7,1,1,a.z)
	spr(a.hat,a.x,a.hat_y,1,1,a.z)
end
