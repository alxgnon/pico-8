pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
-- vania
--
function _init()
	apply_map_texture()
	player=new_player(32,0)
end

function _update()
	update_player(player)
	update_camera(player)
end

function _draw()
	draw_map_for(player)
	draw_player(player)
	camera()
	print(player.land_t)
end

function apply_map_texture()
	for i=0,127 do
		for j=0,63 do
			if mget(i,j)==1 then
				if rnd(99)>94 then
					mset(i,j,3)
				elseif rnd(99)>76 then
					mset(i,j,2)
				end
			end
		end
	end
end

function update_camera(a)
	a.cam_x=min(max(a.x-60,8),896)
	a.cam_y=min(max(a.y,0),384)
	a.cam_y=flr(a.cam_y/128)*128
end

function draw_map_for(a)
	cls()
	camera(a.cam_x,a.cam_y)
	map(0,0,0,0,128,64)
end
-->8
-- player
--

--
-- new_player( x, y )
--
-- creates a player object.
--
function new_player(x,y)
	return{
		t=0,
		f=48,
		x=x,
		y=y,
		dx=0,
		dy=0,
		air_t=10,
		hat=32+flr(rnd(2)),
		cam_x=8,
		cam_y=0
	}
end

--
-- update_player( player )
--
-- runs a frame of player time.
--
function update_player(a)
	horizontal_input(a)
	vertical_input(a)
	horizontal_movement(a)
	vertical_movement(a)
	update_physics(a)
	animate_player(a)
	a.t+=1
end

--
-- horizontal_input( player )
--
-- reads left/right input.
--
function horizontal_input(a)
	if btn(0) and btn(1) then
		a.dx=0
	elseif btn(0) then
		a.dx=-3
		a.z=true
	elseif btn(1) then
		a.dx=3
		a.z=false
	end
end

--
-- vertical_input( player )
--
-- reads jump input.
--
function vertical_input(a)
	local can_jump
	if btn(4) then
		-- jump a bit after falling
		can_jump=a.land or a.air_t<4
		if can_jump and a.release then
				a.land=false
				a.air_t=0
				a.dy=-8
		end
	end
	a.release=not btn(4)
end

--
-- horizontal_movement( player )
--
-- applies left/right movement.
--
function horizontal_movement(a)
	local t,tx,ty
	if(abs(a.dx)<1)return
	-- move pixel by pixel
	for i=1,abs(a.dx) do
		a.x+=sgn(a.dx)
		for j=1,6,5 do
			tx=flr((a.x+j)/8)
			ty=flr(a.y/8)
			t=mget(tx,ty)
			if fget(t,1) then
				-- collided with a wall
				a.x-=sgn(a.dx)
				a.dx=0
				return
			end
		end
	end
end

--
-- vertical_movement( player )
--
-- applies up/down movement.
--
function vertical_movement(a)
	local t,tx,ty
	local oy=a.y
	local land=a.land
	-- move pixel by pixel
	for i=1,abs(a.dy) do
		a.y+=sgn(a.dy)
		for j=1,6,5 do
			tx=(a.x+j)/8
			ty=a.y/8
			t=mget(tx,ty)
			if fget(t,1) then
				-- collided with floor
				a.y-=sgn(a.dy)
				a.land=true
				return
			end
			a.land=false
		end
	end
	-- just started falling
	if land and not a.land then
		a.air_t=0
	end
	-- don't fall for a bit
	if a.air_t<3 and oy<a.y then
		a.y=oy
	end
end

--
-- update_physics( player )
--
--
--
function update_physics(a)
	-- air time
	if not a.land then
		a.air_t+=1
	end
	-- friction
	if a.dx!=0 then
		a.dx-=sgn(a.dx)
	end
	if abs(a.dx)<1 then
		a.dx=0
	end
	a.dy=min(a.dy+1,6)
end

function animate_player(a)
	a.f0=a.f+1
	if a.land and abs(a.dx)>1 then
		a.f0=a.f+flr(a.t/4)%4
	elseif not a.land and a.dy<3 then
		a.f0=a.f+2
	end
	a.hat_y=a.y-(a.f0-1)%2-14
end

function draw_player(a)
	spr(a.f0,a.x,a.y-7,1,1,a.z)
	spr(a.hat,a.x,a.hat_y,1,1,a.z)
end
