world={
	make=function()
		room={}
		for i=1,32 do
			room[i]={}
		end
		_p(1,5)
		_b(2,5)
		_h(4,5)
		_t(6,5)
		_p(1,6)
		_t(4,6)
		_h(1,8)
		_h(3,8)
		_f(5,7)
		_t(6,8)
	end,
	update=function()
	end,
	move=function(x,y)
		x,y=flr(x),flr(y)
		if room[x] and room[x][y] then
			r=room[x][y]
			if(r.l)r.l()
		end
	end,
	draw=function()
		map(r.j,r.i,r.x*128,r.y*128,r.w,r.h)
	end
}

function _r(x,y,j,i,w,h,l)
	return{j=j,i=i,x=x,y=y,w=w,h=h,l}
end

function _b(x,y,l)
	local r=_r(x,y,32,0,32,32,l)
	room[x][y]=r
	room[x+1][y]=r
	room[x][y+1]=r
	room[x+1][y+1]=r
end

function _h(x,y,l)
	local r=_r(x,y,80,0,32,16,l)
	room[x][y]=r
	room[x+1][y]=r
end

function _t(x,y,l)
	local r=_r(x,y,64,16,48,16,l)
	room[x][y]=r
	room[x+1][y]=r
	room[x+2][y]=r
end

function _f(x,y,l)
	local r=_r(x,y,112,0,16,32,l)
	room[x][y]=r
	room[x][y+1]=r
end

function _p(x,y,l)
	local r=_r(x,y,64,0,16,16,l)
	room[x][y]=r
end
