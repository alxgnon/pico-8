function arrows(mult)
	local dx,dy=0,0
	if(btn"0")dx-=mult
	if(btn"1")dx+=mult
	if(btn"2")dy-=mult
	if(btn"3")dy+=mult
	return dx,dy,dx!=0 or dy!=0
end

