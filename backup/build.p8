pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
function _init()
	poke(24365,1)
	brush=0
	t=0
end

function update_mouse()
	mx,my=stat(32),stat(33)
	tx=min(max(flr(mx/8),0),15)
	ty=min(max(flr(my/8),0),15)
	tile=mget(tx,ty)
	mkey=stat(34)
end

function mouse_click()
	if mkey>0 and mup then
		if fget(tile,1) then
			mset(tx,ty,brush)
			brush=tile
		end
	end
	mup=stat(34)==0
	t+=1
end

function _update()
	update_mouse()
	mouse_click()
end

function _draw()
	cls()
	map()
	pset(mx,my,7)
end
