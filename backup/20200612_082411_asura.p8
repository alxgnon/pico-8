-- asura
--
-- init
function _init()
	gt=0
	actors={}
	pl=_deva(0,64)
	add(actors,pl)
end

-- update
function _update()
	gt+=1
	gt+=1
	for a in all(actors) do
		if a==pl then
			a:input()
		end
		a:update()
	end
end

-- draw
function _draw()
	cls(13)
	draw_sky()
	for a in all(actors) do
		a:draw()
	end
end

-- draw sky
function draw_sky()
	local camx,camy=pl:camera()
	camx=min(max(camx,-512),385)
	camera(camx,camy)
	rectfill(-512,200,512,232,4)
	line(-512,200,512,200,11)
	map(0,0,-512,136,128,16)
end
-->8
-- deva
function _deva(x,y)
	return{
		x=x,
		y=y,
		dx=0,
		dy=0,
		flap=0,
		fly=true,
		-- deva:input
		input=function(a)
			if(btn(0))a.dx=-3
			if(btn(1))a.dx=3
			if btn(4) and a.flap<=0 then
				a.flap=1
				a.dy=-14
			elseif not btn(4) and a.dy>=-3 then
				a.flap-=1
			end
		end,
		-- deva:update
		update=function(a)
			a.x+=a.dx
			a.y+=a.dy
			a.dy=a.dy+abs(a.y*0.01)
			a.dx*=0.777
			a.dy*=0.777
			a.x=min(max(a.x,-514),507)
			a.y=min(max(a.y,50),192)
			a.fly=a.y>192
		end,
		-- deva:camera
		camera=function(a)
			return a.x-60,min(a.y-8,100)
		end,
		-- deva:draw
		draw=function(a)
			local f=max(a.flap,-2)
			local x,y=a.x,a.y
			local fx,fy=a.dx/2,a.dy/2
			if a.y<180 then
				rect(x+2,y-4,x+5,y-2,10)
				if	a.flap>-5 then
					spr(max(2+f,0),x-8,y-fx-fy,1,1,1)
					spr(max(2+f,0),x+8,y+fx-fy)
				end
			end
			if not btn(0) and not btn(1) then
				spr(4,x,y)
			elseif not a.fly and btn(0) then
				spr(6+gt%2,x,y)
			elseif not a.fly and btn(1) then
				spr(6+gt%2,x,y,1,1,true)
			elseif btn(0) then
				spr(5,x,y)
			elseif btn(1) then
				spr(5,x,y,1,1,true)
			end
		end
	}
end
