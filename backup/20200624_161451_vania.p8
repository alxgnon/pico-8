version 27
__lua__
-- vania
--
root=0
player=32

init={}
update={}
draw={}

function _init()
	for i=0,127 do
		for j=0,63 do
			if mget(i,j)==1 then
				if rnd(100)>91 then
					mset(i,j,3)
				elseif rnd(100)>76 then
					mset(i,j,2)
				end
			end
		end
	end
	set_transparent(1)
	section=0
	sleep=0
	rumble=0
	actors={}
	game=make(root,0,0,0,0)
	pl=make(player,59,114,1,1)
end

function _update()
	if unsleep() then
		for a in all(actors) do
			a.update(a)
			a.t+=1
		end
	end
end

function _draw()
	cls(0)
	camera(game.x,game.y)
	shadow(pl.x,pl.y)
	for a in all(actors) do
		a.draw(a)
	end
end
