-- entity
--
function make(f,x,y,w,h)
	local a={
		t=0,
		f=f,
		x=x,
		y=y,
		w=w,
		h=h,
		dx=0,
		dy=0,
		update=update[f],
		draw=draw[f]
	}
	init[f](a)
	add(actors,a)
	return a
end

init[root]=function(a)end
update[root]=function(a)end
draw[root]=function(a)
	map(0,0,0,0,128,64)
end

init[player]=function(a)
	a.hat=player-16+flr(rnd(2))
end
update[player]=function(a)
	control(a)
	jump(a)
	move(a)
	fall(a)
	physics(a)
	follow(a.x,a.y)
end
draw[player]=function(a)
	local f=anim_walk(a)
	local y=anim_hat(a,f)
	sprite(a.hat,a.x,y,1,1,a.z)
	sprite(f,a.x,a.y,1,1,a.z)
end
-->8
-- component
--
function control(a)
	if btn(0) and btn(1) then
		a.dx=0
	elseif btn(0) then
		a.dx=-3
		a.z=true
	elseif btn(1) then
		a.dx=3
		a.z=false
	end
end

function jump(a)
	a.fastfall=btn(3) and a.dy>5
	if btn(4) and not btn(3) then
		if a.land and a.rel then
			a.land=false
			a.dy=-7
		end
		a.rel=false
	else
		a.rel=true
	end
end

function move(a)
	if(abs(a.dx)<1)return
	for i=1,abs(a.dx) do
		a.x+=sgn(a.dx)
		for j=1,6,5 do
			local t=mget((a.x+j)/8,a.y/8)
			if fget(t,1) then
				a.x-=sgn(a.dx)
				a.dx=0
				return
			end
		end
	end
end

function fall(a)
	for i=1,abs(a.dy) do
		a.y+=sgn(a.dy)
		for j=1,6,5 do
			t=mget((a.x+j)/8,a.y/8)
			if fget(t,1) then
				a.y-=sgn(a.dy)
				a.land=true
				return
			end
			a.land=false
		end
		if a.dy<0 then
			for j=1,6,5 do
				t=mget((a.x+j)/8,(a.y-8)/8)
				if fget(t,1) then
					a.y-=sgn(a.dy)
					return
				end
			end
		end
	end
end

function physics(a)
	if(a.dx!=0)a.dx-=sgn(a.dx)
	if(abs(a.dx)<1)a.dx=0
	if a.fastfall then
		a.dy=14
	else
		a.dy=min(a.dy+1,6)
	end
end

function follow(x,y)
	local camx,camy=0,0
	local sect=0
	if x<384 then
		camx=min(max(x-60,0),256)
		camy=flr(y/256)*256
		camy=min(max(y-88,camy),camy+128)
		camy=max(camy,31)
	elseif x<896 then
		camx=min(max(x-60,384),768)
		camy=flr(y/128)*128
		sect=1
	else
		camx=min(max(x-60,896),896)
		camy=flr(y/128)*128
		sect=2
	end
	if section!=sect then
		section=sect
		sleep=6
	else
		game.x=camx
		game.y=camy
	end
end

function anim_walk(a)
	local f=a.f+1
	if a.land then
		if abs(a.dx)>1 then
			f=a.f+(a.t/4)%4
		end
	elseif a.dy<3 then
		f=a.f+2
	end
	return flr(f)
end

function anim_hat(a,f)
	local dy=(f%2==0) and 1 or 0
	return a.y-dy-7
end

function sprite(f,x,y,w,h,z)
	spr(f,x,y-h*8+1,w,h,z)
end

function shadow(x,y)
	if y>256 then
		circfill(x+3,y-4,24,1)
	end
end

function set_transparent(value)
	palt(0,false)
	palt(value,true)
end

function unsleep()
	if(sleep>0)sleep-=1
	return sleep<=0
end
