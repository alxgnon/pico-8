pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
function _init()
	x=7
	y=6
	t=0
end

function _update()
	t-=1
	local dx=0
	if(btn"0")dx-=1
	if(btn"1")dx+=1
	if dx!=0 and t<=0 and
		mget(x+dx,y+1)==1 then
			mset(x,y,0)
			x+=dx
			mset(x,y,2)
			t=2
	end
	if btn"4" then
		if mget(x-1,y)==3 then
			mset(x-1,y,5 )
			mset(x-1,y-1,3)
		end
	end
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
end
