__lua__
-- vania
--
function _init()
	palt(0,false)
	palt(1,true)
	actors={}
	game=make(root,0,0,128,64)
	pl=make(player,60,230,1,1)
end

function _update()
	for a in all(actors) do
		a.update(a)
		a.t+=1
	end
end

function _draw()
	cls(0)
	camera(game.x,game.y)
	circfill(pl.x+3,pl.y-4,40,1)
	for a in all(actors) do
		a.draw(a)
	end
end
-->8
-- entity
--
root=0
player=32

init={}
update={}
draw={}

function make(f,x,y,w,h)
	local a={f=f,x=x,y=y,w=w,h=h,dx=0,dy=0,t=0}
	local init=init[f]
	a.update=update[f]
	a.draw=draw[f]
	init(a)
	add(actors,a)
	return a
end

init[root]=function(a)
end

update[root]=function(a)
end

draw[root]=function(a)
	camera(a.x,a.y)
	map(0,0,0,0,128,64)
end

init[player]=function(a)
	a.hat=player-16+flr(rnd(8))
end

update[player]=function(a)
	control(a)
	move(a)
	fall(a)
	physics(a)
	follow(a.x,a.y)
end

draw[player]=function(a)
	local f=anim_walk(a)
	local dy=f%2==0 and 1 or 0
	sprite(a.hat,a.x,a.y-7-dy,1,1,a.z)
	sprite(f,a.x,a.y,1,1,a.z)
end
-->8
-- component
--
function control(a)
	if btn(0) and btn(1) then
		a.dx=0
	elseif btn(0) then
		a.dx=-3
		a.z=true
	elseif btn(1) then
		a.dx=3
		a.z=false
	end
	if btn(4) then
		if a.land and a.rel then
			a.land=false
			a.dy=-7
		end
		a.rel=false
	else
		a.rel=true
	end
end

function move(a)
	if(abs(a.dx)<1)return
	for i=1,abs(a.dx) do
		a.x+=sgn(a.dx)
		for j=1,6,5 do
			local t=mget((a.x+j)/8,a.y/8)
			if fget(t,1) then
				a.x-=sgn(a.dx)
				a.dx=0
				return
			end
		end
	end
end

function fall(a)
	for i=1,abs(a.dy) do
		a.y+=sgn(a.dy)
		for j=1,6,5 do
			t=mget((a.x+j)/8,a.y/8)
			if fget(t,1) then
				a.y-=sgn(a.dy)
				a.land=true
				return
			end
			a.land=false
		end
	end
end

function physics(a)
	if(a.dx!=0)a.dx-=sgn(a.dx)
	if(abs(a.dx)<1)a.dx=0
	a.dy=min(a.dy+1,8)
end

function follow(x,y)
	local camx,camy=0,0
	if x<384 then
		camx=min(max(x-60,0),256)
		camy=flr(y/256)*256
		camy=min(max(y-88,camy),camy+128)
	elseif x<896 then
		camx=min(max(x-60,384),768)
		camy=flr(y/128)*128
	else
		camx=min(max(x-60,896),896)
		camy=flr(y/128)*128
	end
	if game.x!=camx or game.y!=camy then
		game.x,game.y=camx,camy
	end
end

function anim_walk(a)
	local f=a.f+1
	if a.land then
		if abs(a.dx)>1 then
			f=a.f+(a.t/4)%4
		end
	elseif a.dy<3 then
		f=a.f+2
	end
	return flr(f)
end

function sprite(f,x,y,w,h,z)
	spr(f,x,y-h*8+1,w,h,z)
end
