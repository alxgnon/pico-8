pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
-- vania
--
function _init()
	state=game()
end

function _update()
	update_game(state)
end

function _draw()
	draw_game(state)
end

-- game -----------------------

function game()
	return{
		world=world(128,64),
		hero=hero(32,0),
		cam={x=8,y=0}
	}
end

function update_game(a)
	update_hero(a.hero)
	update_cam(a.cam,a.hero)
end

function draw_game(a)
	cls()
	draw_cam(a.cam)
	draw_world(a.world)
	draw_hero(a.hero)
end

-- world ----------------------

function world(w,h)
	prepare_world(w,h)
	return{w=w,h=h}
end

function prepare_world(w,h)
	for i=0,w-1 do
		for j=0,h-1 do
			if mget(i,j)==1 then
				if rnd(99)>94 then
					mset(i,j,3)
				elseif rnd(99)>76 then
					mset(i,j,2)
				end
			end
		end
	end
end

function draw_world(a)
	map(0,0,0,0,a.w,a.h)
end

-- cam ------------------------

function update_cam(a,b)
	a.x=min(max(b.x-48,8),896)
	a.y=flr(b.y/128)*128
end

function draw_cam(a)
	camera(a.x,a.y)
end
-->8
-- hero -----------------------

function hero(x,y)
	return{
		x=x,
		y=y,
		f=48,
		dx=0,
		dy=0,
		t=0,
		air_t=10,
		hat=32+flr(rnd(4))
	}
end

function update_hero(a)
	player_input(a)
	update_physics(a)
	animate_hero(a)
	physics_timer(a)
end

-- render ---------------------

function animate_hero(a)
	a.f_y=a.y-(flr(a.t/12)-1)%2-10
	a.hat_y=a.f_y-8
end

function draw_hero(a)
	spr(a.f,a.x,a.f_y,1,1,a.z)
	spr(a.hat,a.x,a.hat_y,1,1,a.z)
end

-- physics --------------------

function update_physics(a)
	horiz_movement(a)
	vert_movement(a)
	apply_friction(a)
	apply_gravity(a)
end

function horiz_movement(a)
	if abs(a.dx)>=1 then
		a.x=collide(a,a.x,a.dx)
	end
end

function vert_movement(a)
	local oy=a.y
	local land=a.land
	a.y=collide(a,a.y,a.dy,true)
	if land and not a.land then
		a.air_t=0
		a.dy=3
	end
	if a.air_t<3 and oy<a.y then
		a.y=oy
	end
end

function apply_friction(a)
	if abs(a.dx)>=1 then
		a.dx-=sgn(a.dx)
	else
		a.dx=0
	end
end

function apply_gravity(a)
	a.dy=min(a.dy+1,6)
end

function physics_timer(a)
	a.t+=1
	if not a.land then
		a.air_t+=1
	end
end

-- collision ------------------

function collide(a,v,dv,vert)
	local tx,ty,tile
	for i=1,abs(dv) do
		v+=sgn(dv)
		for j=1,6,5 do
			tx,ty=curr_tile(a,v,j,vert)
			tile=mget(tx,ty)
			if fget(tile,1) and vert then
				a.land=true
				return v-sgn(dv)
			elseif fget(tile,1) then
				a.dx=0
				return v-sgn(dv)
			elseif vert then
				a.land=false
			end
		end
	end
	return v
end

function curr_tile(a,v,j,vert)
	local tx,ty
	if vert then
		tx=flr((a.x+j)/8)
		ty=flr(v/8)
	else
		tx=flr((v+j)/8)
		ty=flr(a.y/8)
	end
	return tx,ty
end

-- input ----------------------

function player_input(a)
	walk_input(a)
	jump_input(a)
	a.release=not btn(4)
end

function walk_input(a)
	if btn(0) and btn(1) then
		a.dx=0
	elseif btn(0) then
		a.dx=-5
		a.z=true
	elseif btn(1) then
		a.dx=5
		a.z=false
	end
end

function jump_input(a)
	if btn(4) and a.release then
		if can_jump(a) then
				a.land=false
				a.air_t=0
				a.dy=-8
		end
	end
end

function can_jump(a)
	return a.land or a.air_t<4
end
