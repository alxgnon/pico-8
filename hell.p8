gfx =
{ jesus = 001
, totem = 007
, ball = 009
, ring = 017
, heart = 012
}

-- roman
roman =
{"","i","ii","iii","iv","v"
,"vi","vii","viii","ix"}

function romanize(n)
	if (n == 1) return "hell"
	local str = roman[n%10]
	for i=1,n/10 do
		str = "x"..str
	end
	return str
end

function pallergy(a, x,y)
	if pget(x,y) == 8 then
		if not a.dead then
			a.dead = true
			gamestate.fadeout = 288
		end
	end
end

function allergy(a)
 local x,y = a.x,a.y
	for j=y+1,y+5 do
		pallergy(a, x+3,j)
	end
	pallergy(a, x+2,y+2)
	pallergy(a, x+4,y+2)
end


------------------ jesus

function jesus(x, y)
	local a =
	{ f = gfx.jesus
	, x = x, y = y
	, speed = 2
	, power = 100
	, aura = aura(x, y)
	, move = move_jesus
	, draw = draw_jesus
	}

	return a
end

function move_jesus(a)
	if a.power <= 126 then
		a.power += 1
	end
	
	if (a.power < 0) a.power = 0

	a.x = max(min(a.x,123),-2)
	a.y = max(a.y,-1)

	a.aura:move(a.x,a.y)

	if a.y > 127 then
		descend()
	end
end

function draw_jesus(a)
	a.aura:draw()
	if a.four then
		spr(018,a.x,a.y)
	else
	spr(a.f,a.x,a.y)
	end
end


------------------------- aura

function aura(x, y)
	local a =
	{ tail = build_tail(x, y)
	, move = move_aura
	, draw = draw_aura
	}

	return a
end

function build_tail(x, y)
	tail = {}

	for i = 1, 4 do
		add(tail,
		{ f = gfx.jesus + i
		, x = x, y = y
		})
	end

	return tail
end

function move_aura(a, x, y)
	for b in all(a.tail) do
		local tx, ty = b.x, b.y
		b.x, b.y = x, y
		x, y = tx, ty
	end
end

function draw_aura(a)
	for b in all(a.tail) do
		spr(b.f, b.x, b.y)
	end
end


---------- blade

function _blade(x,y)
	return
	{ x=x,y=y
	, dy = 4

	, move =
	function(a)
		a.y += a.dy

		for b in all(gamestate.totems) do
			if a.y > b.y and
					a.x >= b.x-3 and
					a.x < b.x + 12 then
				del(gamestate.blades, a)
				b.hp -= 1
				sfx(3)
				shake(b, 8, 3)

				if b.hp <= 0 then
					sfx(4)
					del(gamestate.totems,b)
					if b.f == gfx.heart then
						local x, y = a.x, a.y
						add(gamestate.balls,_ball(x-4,y+2))
						add(gamestate.balls,_ball(x,y))
						add(gamestate.balls,_ball(x+4,y))
						add(gamestate.balls,_ball(x+8,y+2))
					
						for d=-1,1,0.02 do
							add(gamestate.rings,{x=x,y=y,ng=d})
						end
					end
					rumble(8, 3)
					return
				end
				
				if not b.awake then
					rumble(8, 3)
					for b in all(gamestate.totems) do
						b.awake = true
					end
				end
				return
			end
		end

		if a.y > 112 and
				not gamestate.chain.dt then
			del(gamestate.blades, a)
			add(gamestate.balls, _ball(a.x,a.y))
			sfx(1)
		end
	end

	, draw =
	function(a)
		spr(006,a.x,a.y,1,2)
	end
	}
end

function heart(x, y)
	local a =
	{ f = gfx.heart
	, x = x
	, y = y
	, hp = 6
	, oy = y
	, t = 0
	, move = move_heart
	, draw = draw_heart
	}

	return a
end

function move_heart(a)
		a.t += 1
		a.y = a.oy + sin(a.t * 0.012) * 1.9
end

function draw_heart(a)
	local ox, oy = 0, 0
	if gt < (a.shake_stop or 0) then
		ox=rnd(a.shake_power)-a.shake_power/2
		oy=rnd(a.shake_power)-a.shake_power/2
		pal(2, 8)
	end

	spr(a.f,a.x+ox,a.y+oy,2,2)
	palette()
end

function shake(a, time, power)
	a.shake_stop = gt + time
	a.shake_power = power
end

function _totem(x,y)
	return
	{ x=x,y=y
	, t=0,oy=y
	, hp = 13

	, hurt =
	function(a)
		a.hp -= 1
	end

	, move =
	function(a)
		a.t += 1

		if a.awake then
			a.y = a.oy + sin(a.t * 0.024) * 2.9
			if a.t % 12 == 0 then
				local b = {x=a.x+6,y=a.y}
				local jc = gamestate.jc
				b.ng = atan2(jc.x-b.x,jc.y-b.y)
				add(gamestate.rings, b)
				add(gamestate.rings, {x=b.x,y=b.y,ng=b.ng-0.1})
				add(gamestate.rings, {x=b.x,y=b.y,ng=b.ng+0.1})
			end
		end

		a.y = a.oy + sin(a.t * 0.012) * 1.9
	end

	, draw =
	function(a)
		local ox, oy = 0, 0
 	if gt < (a.shake_stop or 0) then
 		ox=rnd(a.shake_power)-a.shake_power/2
 		oy=rnd(a.shake_power)-a.shake_power/2
 		pal(2, 8)
 	end

		if a.awake then
			spr(gfx.totem+3,a.x+ox,a.y+oy,2,2)
		else
			spr(gfx.totem,a.x+ox,a.y+oy,2,2)
		end
		palette()
	end
	}
end

function _ball(x,y)
	return
	{ x=x,y=y
	, dy = -4

	, move =
	function(a)
		a.y += a.dy
		if a.y < -16 then
			del(gamestate.balls, a)
		end
	end

	, draw =
	function(a)
		spr(gfx.ball,a.x,a.y,1,2)
	end
	}
end

function _chain()
	return
	{ x = 0

	, move =
	function(a)
		a.x = (a.x + 1) % 8

		if #gamestate.totems < 1 then
			a.dt = (a.dt or 0) + 4
		end
	end

	, draw =
	function(a)
		if a.dt and a.dt >0 then
			map(0,0,64+a.dt,118,8,1)
			map(0,0,-a.dt,118,8,1)
		else
			map(0,0,-a.x,118,17,1)
		end
	end
	}
end
