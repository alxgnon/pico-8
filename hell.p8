-- next level
function descend()
	gamestate.fadeout = 144
end

function _level(n)
	local lvl =
	{ n = n
	, x = flr(n / 4) * 16
	, y = n % 4 * 16
	, fadein = 144 }

	lvl.jc = jesus(60,16)
	lvl.blades = {}
	lvl.balls = {}

	lvl.totems = {}
	lvl.rings = {}

	for i=0,15 do
		for j=0,15 do
			local f = mget(lvl.x+i,lvl.y+j)
			if fget(f, 1) then
				local x, y = i*8, j*8
				if f == gfx.totem then
					add(lvl.totems, _totem(x,y))
				elseif f == gfx.heart then
					add(lvl.totems, heart(x,y))
				end
			end
		end
	end

	lvl.chain = _chain()

	function lvl:move()
		local jc = self.jc

		if not btn"4" then
			jc.four = false
		end

		pad.control(jc)

		if (jc.four) return

		if self.fadein > 0 then
			self.fadein -= 16
			return
		elseif self.fadeout and
				self.fadeout > 0 then
			self.fadeout -= 16
			if self.fadeout <= 0 then
				local level = self.n+1
				if self.jc.dead then
					level -= 1
				end
				gamestate = _level(level)
			end
			return
		end

		if (self.jc.dead) return

		for a in all(self.blades) do
			a:move()
		end

		for a in all(self.balls) do
			a:move()
		end

		for a in all(lvl.totems) do
			a:move()
		end

		for a in all(lvl.rings) do
			a.x += cos(a.ng)
			a.y += sin(a.ng)
		end

		self.jc:move()
		if self.chain then
			self.chain:move()
		end
	end

	function lvl:draw()
		cls()
		palette()
		rectfill(1,1,self.jc.power,9,10)
		rect(0,0,127,180,2)
		print(romanize(lvl.n+1),3,3,0)
		self.jc:draw()
		map(self.x,self.y,0,0,16,16,1)
		for i=#self.blades,1,-1 do
			self.blades[i]:draw()
		end

		for a in all(self.balls) do
			a:draw()
		end

		for a in all(lvl.totems) do
			a:draw()
		end

		for a in all(lvl.rings) do
			spr(gfx.ring,a.x,a.y)
		end


		if self.chain then
			self.chain:draw()
		end
		allergy(self.jc)

		if self.fadein > 0 then
			rectfill(0,0,127,self.fadein,0)
		elseif self.fadeout and
				self.fadeout > 0 then
			rectfill(0,self.fadeout,127,127,0)
		end
	end

	return lvl
end
