---------------------- pad ----

pad = {}

function pad.get_delta(speed)
	local dx,dy = 0,0
	if (btn"0") dx -= speed
	if (btn"1") dx += speed
	if (btn"2") dy -= speed
	if (btn"3") dy += speed
	return dx,dy
end

function pad.control(a)
	if (gamestate.fadein > 0) return
	if (gamestate.fadeout) return

	if (a.dead) return
	if (a.four and a.four < 1) return

	if not a.four and btn"4" then
		a.four = 6
		gamestate.blades = {}
	end

	local speed = 1
	if a.four then
		if a.power < 1 then
			return
		end
		speed *= 2
	end

	local dx,dy = pad.get_delta(a.speed)
	a.x += dx * speed
	a.y += dy * speed

	if a.four then
		a.power -= 7
		if dx != 0 or dy != 0 then
			a.four -= 1
		end
		if not gamestate.chain.dt then
			a.y = min(a.y,112)
		end
		return
	end

	if not btn"5" then
		a.five = false
	elseif not a.five and btn"5" then
		if (a.power < 1) return
		a.five = true
		add(gamestate.blades,
		_blade(a.x,a.y))
		sfx(2)
		a.st = 0
		a.power -= 16
	else
		if (a.power < 1) return
		if a.st % 6 == 0 then
			add(gamestate.blades,
			_blade(a.x,a.y))
		sfx(5)
		end
		a.st += 1
		a.power -= 2
	end
end


