-- next level
function descend()
	gamestate.fadeout = 144
end

function _level(n)
	local lvl =
	{ n = n
	, x = flr(n / 4) * 16
	, y = n % 4 * 16
	, fadein = 144 }

	lvl.jc = jesus(60,4)
	lvl.blades = {}

	lvl.totems = {}
	lvl.rings = {}

	lvl.beams = {}

	lvl.dust = {}

	for i=0,15 do
		for j=0,15 do
			local f = mget(lvl.x+i,lvl.y+j)
			if fget(f, 1) then
				local x, y = i*8, j*8
				if f == gfx.totem then
					add(lvl.totems, _totem(x,y))
				elseif f == gfx.heart then
					add(lvl.totems, heart(x,y))
				end
			end
		end
	end

	lvl.chain = _chain()

	function lvl:move()
		local jc = self.jc

		if not btn"4" then
			jc.four = false
		end

		pad.control(jc)

		if (jc.four) return

		if self.fadein > 0 then
			self.fadein -= 16
			return
		elseif self.fadeout and
				self.fadeout > 0 then
			self.fadeout -= 16
			if self.fadeout <= 0 then
				local level = self.n+1
				if self.jc.dead then
					level -= 1
				end
				gamestate = _level(level)
			end
			return
		end

		if (self.jc.dead) return

		for a in all(self.blades) do
			a:move()
		end

		for a in all(lvl.totems) do
			a:move()
		end

		for a in all(lvl.rings) do
			a.x += cos(a.ng)
			a.y += sin(a.ng)
			a.t = (a.t or rnd(4)) + 1
		end

		for a in all(lvl.beams) do
			a:move()
		end

		self.jc:move()
		if self.chain then
			self.chain:move()
		end

		move_dust()
	end

	function lvl:draw()
		if not self.jc.dead then
		cls()
		palette()

		rect(0,0,127,180,1)

		for a in all(lvl.beams) do
			a:draw()
		end

		draw_dust()

		for i=#self.blades,1,-1 do
			self.blades[i]:draw()
		end

		self.jc:draw()

		if self.chain then
			self.chain:draw()
		end

		if self.jc.four then
			pal(8, 9)
		end
		for a in all(lvl.rings) do
			local off = (a.t/ 3) % 4
			spr(gfx.ring + off,a.x,a.y)
		end
		palette()


		for a in all(lvl.totems) do
			a:draw()
		end

		allergy(self.jc)
		else -- dead
			for i = 0, 800 do
			pset(rnd(128),rnd(128), 0)
			end
		end

		if self.fadein > 0 then
			rectfill(0,0,127,self.fadein,0)
		elseif self.fadeout and
				self.fadeout > 0 then
			rectfill(0,self.fadeout,127,127,0)
		end
	end

	return lvl
end
