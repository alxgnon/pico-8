function palette()
	pal()
	palt(0, false)
	palt(12, true)
end

-- rumble ---------------------

function rumble(time, power)
	rumble_stop = gt + time
	rumble_power = power
end

function draw_rumble()
	if gt < (rumble_stop or 0) then
		camera(
		rnd(rumble_power)-rumble_power/2,
		rnd(rumble_power)-rumble_power/2)
	elseif rumble_stop then
		camera(0,0)
		rumble_stop = nil
	end
end

---------------- callbacks ----
function _init()
	palette()
	gamestate = _level(0)
	gt = 0
end

function _update()
	gt += 1
	gamestate:move()
end

function _draw()
	draw_rumble()
	gamestate:draw()
end
